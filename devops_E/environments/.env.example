# Example Environment Variables - Copy to .env.dev and fill with real values

# Database
POSTGRES_HOST=database
POSTGRES_PORT=5432
POSTGRES_DB=transcendence
POSTGRES_USER=transcendence
POSTGRES_PASSWORD=your_secure_password

# Backend
NODE_ENV=development
PORT=3000
JWT_SECRET=your_super_secure_jwt_secret
JWT_EXPIRES_IN=24h

# Frontend
VITE_API_URL=http://localhost:3000
VITE_APP_TITLE=Transcendence

# OAuth Configuration
OAUTH_42_CLIENT_ID=your_42_client_id
OAUTH_42_CLIENT_SECRET=your_42_client_secret
OAUTH_42_CALLBACK_URL=http://localhost:3000/auth/42/callback

GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret

# Redis
REDIS_HOST=redis
REDIS_PORT=6379

# Monitoring
PROMETHEUS_ENABLED=true
GRAFANA_ADMIN_PASSWORD=secure_admin_password
